{
  "name": "jscodeshift-x",
  "version": "0.16.1",
  "description": "A toolkit for JavaScript codemods",
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/jscodeshift.git"
  },
  "bugs": "https://github.com/facebook/jscodeshift/issues",
  "main": "index.js",
  "scripts": {
    "prepare": "cp -R src/ dist/",
    "test": "jest --bail",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --fix --max-warnings=0",
    "docs": "rm -rf docs && jsdoc -d docs -R README.md src/collections/* src/core.js src/Collection.js"
  },
  "bin": {
    "jscodeshift": "./bin/jscodeshift.js"
  },
  "keywords": [
    "codemod",
    "recast",
    "babel"
  ],
  "author": "Pionxzh",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.24.7",
    "@babel/parser": "^7.24.7",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.7",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.24.7",
    "@babel/plugin-transform-optional-chaining": "^7.24.7",
    "@babel/plugin-transform-private-methods": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/register": "^7.24.6",
    "picocolors": "^1.0.1",
    "graceful-fs": "^4.2.4",
    "micromatch": "^4.0.7",
    "neo-async": "^2.5.0",
    "node-dir": "^0.1.17",
    "recast": "^0.23.9",
    "temp": "^0.9.4",
    "write-file-atomic": "^5.0.1"
  },
  "peerDependencies": {
    "@babel/preset-env": "^7.1.6"
  },
  "peerDependenciesMeta": {
    "@babel/preset-env": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/eslint-parser": "^7.24.8",
    "eslint": "8.57.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3"
  },
  "jest": {
    "roots": [
      "src",
      "bin",
      "parser",
      "sample"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.15.7"
}
